{
    "name": "just-some-code/flysystem-vault",
    "description": "Flysystem Adapter for Hashicorp Vault",
    "type": "library",
    "require": {
        "php": "^8.1",
        "csharpru/vault-php": "^4.3",
        "alextartan/guzzle-psr18-adapter": "^4.0",
        "laminas/laminas-diactoros": "^2.22"
    },
    "require-dev": {
        "illuminate/support": "^9.42",
        "phpunit/phpunit": "^9.5",
        "league/flysystem-adapter-test-utilities": "^3.10",
        "illuminate/http": "^9.42"
    },
    "license": "Private",
    "autoload": {
        "psr-4": {
            "JustSomeCode\\FlysystemVault\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nikola Bursac",
            "email": "nikola.bursac@proton.me"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
